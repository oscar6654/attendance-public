<h1><b>Post Delivery Status</b></h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for(@invoice6_edit, url: admin_update_invoice_status_invoice_path(@invoice6_edit)) do |f| %>
      <div class="form-group">
        <label>Post Delivery Status</label>
        <%= f.select :post_status, [["Opened","Opened"],["Closed","Closed"]], {:prompt => true, :multiple=>true } %>
      </div>
      
      
      
      <div class="form-group">
        <%= f.submit "Post Delivery Status", class: 'btn btn-success' %>
      </div>
    <%end%>
  </div>
</div>