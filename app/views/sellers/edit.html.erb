<h1><b>Edit Memo</b></h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for(@seller_edit, url: seller_path(@seller_edit)) do |f| %>
      <div class="form-group">
        <label>Name</label>
        <%= f.text_field :name, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.submit "Edit Seller", class: 'btn btn-success' %>
      </div>
    <%end%>
  </div>
</div>