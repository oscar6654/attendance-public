<h1><b>Create Discount Codes</b></h1>
<div class="row">
  <div class="col-md-8">
    <%= simple_form_for(@code1, url: codes_path) do |f| %>
      <div class="form-group">
        <label>Name</label>
        <%= f.text_field :name, autofocus: true, class: 'form-control'%>
      </div>
      

      
      <div class="form-group">
        <%= submit_tag "Create Discount Code", class: 'btn btn-success' %>
      </div>
    <%end%>
  </div>
</div>