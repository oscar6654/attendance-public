<div class = "container">
<h1><b> Codes</b></h1>
<table id = "working_hours1">
    <thead>
      <tr>
        <th>ID</th>
        <th data-sortable="true">Code Name</th>
        <th date-sortable="true">Created At</th>
      </tr>
    </thead>
    <tbody>
      <% @code.each do |user| %>
      <tr>
        <td><%=user.id %></td>
        <td><%= link_to user.name, user %></td>
        <td><%= (user.created_at).strftime("%m/%d/%Y") %></td>
      </tr>
      <%end%>
    </tbody>
</table>
</div>